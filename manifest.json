{
  "name": "Youtube",
  "version": "1.0.0",
  "manifest_version": 2,
  "minimum_chrome_version": "22",
  "description": "Play embedded Youtube videos without problems!",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.youtube.com/"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}